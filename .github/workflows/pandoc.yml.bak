on: [push, pull_request]

name: Pandoc

jobs:
  pandoc:
    strategy:
      matrix:
        os: [macOS-latest, ubuntu-latest, windows-latest]
    runs-on: ${{ matrix.os }}
    steps:
      - uses: r-lib/actions/setup-pandoc@master
      - run: |
          echo "# hi" > test.md
          pandoc test.md -t html
